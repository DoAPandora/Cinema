{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Cinema",
    "id": "cinema",
    "version": "0.0.1",
    "url": "https://github.com/bsq-ports/Cinema",
    "additionalData": {
      "overrideSoName": "libcinema.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "logcat": [
        "adb logcat -c",
        "pwsh ./scripts/start-logging.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 --release",
        "qpm qmod manifest",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1"
      ]
    }
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.0.0",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "bs-events",
      "versionRange": "^0.0.5",
      "additionalData": {}
    },
    {
      "id": "pythonlib",
      "versionRange": "^0.3.2",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^1.4.1",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^3.6.3",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "<=0.4.24",
      "additionalData": {}
    },
    {
      "id": "rapidjson-macros",
      "versionRange": "^1.2.1",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3500.0.0",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.3",
      "additionalData": {}
    },
    {
      "id": "songcore",
      "versionRange": "^1.1.9",
      "additionalData": {}
    }
  ]
}