{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Cinema",
    "id": "cinema",
    "version": "0.0.1",
    "url": "https://github.com/bsq-ports/Cinema",
    "additionalData": {
      "overrideSoName": "libcinema.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "a2l": [
        "pwsh ./scripts/addr2line.ps1 libcinema.so $0"
      ],
      "build": [
        "pwsh ./scripts/build.ps1 --release --buildFolder build"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 --useDebug"
      ],
      "debug": [
        "pwsh ./scripts/build.ps1 --buildFolder build-debug"
      ],
      "format": [
        "find ./src -name *.cpp -exec clang-format -i {} ;"
      ],
      "push": [
        "adb push ./build/libcinema.so /sdcard/moddata/com.beatgames.beatsaber/modloader/mods"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 -analyze",
        "code RecentCrash.log_processed.log"
      ]
    },
    "ndk": "^27.1.12297006",
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs",
      "third_party/cpython/lib"
      "libcrypto_python.so",
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "Cinema"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^2.0.3",
      "additionalData": {}
    },
    {
      "id": "rapidjson-macros",
      "versionRange": "^2.1.0",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^4008.*",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.55",
      "additionalData": {}
    },
    {
      "id": "songcore",
      "versionRange": "^1.1.24",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "sombrero",
      "versionRange": "^0.1.43",
      "additionalData": {}
    },
    {
      "id": "hollywood",
      "versionRange": "^1.2.2",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {}
    },
    {
      "id": "metacore",
      "versionRange": "^1.5.0",
      "additionalData": {}
    }
  ]
}
