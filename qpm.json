{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Cinema",
    "id": "cinema",
    "version": "0.0.1",
    "url": "https://github.com/bsq-ports/Cinema",
    "additionalData": {
      "overrideSoName": "libcinema.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "a2l": [
        "pwsh ./scripts/addr2line.ps1 libcinema.so $0"
      ],
      "build": [
        "pwsh ./scripts/build.ps1 --release"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "debug": [
        "pwsh ./scripts/build.ps1"
      ],
      "format": [
        "find ./src -name *.cpp -exec clang-format -i {} ;"
      ],
      "push": [
        "adb push ./build/libcinema.so /sdcard/moddata/com.beatgames.beatsaber/modloader/mods"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 -analyze",
        "code RecentCrash.log_processed.log"
      ]
    },
    "ndk": "^27.1.12297006",
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "Cinema"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.0.0",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "bs-events",
      "versionRange": "^1.1.0",
      "additionalData": {}
    },
    {
      "id": "pythonlib",
      "versionRange": "^0.4.0",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^1.4.1",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^3.6.3",
      "additionalData": {}
    },
    {
      "id": "rapidjson-macros",
      "versionRange": "^1.2.1",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3700.0.0",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.3",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.42",
      "additionalData": {}
    },
    {
      "id": "songcore",
      "versionRange": "^1.1.13",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "sombrero",
      "versionRange": "^0.1.42",
      "additionalData": {}
    }
  ]
}